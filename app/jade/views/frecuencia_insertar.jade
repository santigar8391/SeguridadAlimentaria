div.modal.fade
  div.modal-dialog
    div.modal-content
      div.modal-header
        button.close(type="button", ng-click="close('Cancel')", data-dismiss="modal", aria-hidden="true")&times;
        h1 Agregar Frecuencia

      div.modal-body
        label Descripcion:
        input(type="text", ng-model="frecuenciaDescripcion", required)
        br
        br
        label Coeficiente:
        input(type="text", ng-model="frecuenciaCoeficiente", required, ng-)
      div.modal-footer
        button.btn.btn-success(ng-click="asignarDatos()") Guardar
        button.btn.btn-danger(type="button", ng-click="close('CLOSED')", data-dismiss="modal", aria-hidden="true")&times; Cancelar
